<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h2><span class="member-name"></span>'s Collector Cache</h2>
    </div>
  </div>
</div>

<br>

<div class="container">

  <div class="row">

    <div class="col-md-6">
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" type="search" placeholder="Card Name" aria-label="Search">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search in your Collection</button>
      </form>
    </div>

    <div class="col-md-6">
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" type="search" placeholder="Card Name" aria-label="Search">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search to Add</button>
      </form>
    </div>

  </div>

</div>

<!-- Modal Button -->
<button id="myBtn">Open Modal</button>

<!-- The Modal -->
<div id="myModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content">
    <div class="modal-header">
      <span class="close">&times;</span>
      <h2>Modal Header</h2>
    </div>
    <div class="modal-body">
      <p>Some text in the Modal Body</p>
      <p>Some other text...</p>
    </div>
    <div class="modal-footer">
      <h3>Modal Footer</h3>
    </div>
  </div>

</div>

<div>
  <div>
  {{#each cards}}
      {{Card-Database}}
  {{/each}}
</div>
</div>